	<meta name="apple-mobile-web-app-status-bar-style" content="black">
	<meta name="apple-mobile-web-app-title" content="Hello World">
	<meta name="msapplication-TileImage" content="public/images/hello-icon-144.png">
	<meta name="msapplication-TileColor" content="#FFFFFF">

	
	<link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

	<link href="https://fonts.googleapis.com/css?family=Lato:300,400,700&display=swap" rel="stylesheet">
	<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
	<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.3.1/dist/css/bootstrap.min.css"
		integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
	<link rel="stylesheet" href="/public/css/loginstyle.css">
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>

<script
  src="https://code.jquery.com/ui/1.13.1/jquery-ui.min.js"
  integrity="sha256-eTyxS0rkjpLEo16uXTS0uVCS4815lc40K2iVpWDvdSY="
  crossorigin="anonymous"></script>

	<script src="https://cdn.jsdelivr.net/npm/popper.js@1.14.7/dist/umd/popper.min.js"
		integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1"
		crossorigin="anonymous"></script>
	<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.3.1/dist/js/bootstrap.min.js"
		integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM"
		crossorigin="anonymous"></script>




	<!-- <script src="https://www.paypal.com/sdk/js?client-id=AUR_R2s32CVCHbACBsi2_YcZVYKu1rgBreGL1dD5uAUgyOcrgK-NeEzwmzE3WA3s5UxpzMVQZ9W-NvG4&currency=USD"></script> -->
	<script src="https://www.paypal.com/sdk/js?client-id=<%=CLIENT_ID%>&currency=USD"></script>


	<script src="/public/js/main.js"></script>
	<script src="/public/js/paypal-payment.js"></script>

	<script>

		if (window.history.replaceState) {
			window.history.replaceState(null, null, window.location.href);
		}

	</script>
</head>

<body>




	<section class="ftco-section">
		<div class="container" id="reportpage">
			<div class="row justify-content-center">
				<!-- <div class="col-md-6 text-center mb-5">
					<h2 class="heading-section">AutoCheck Report </h2> <a
						href="/public/report/sample_autocheck_report.html" target="_blank" style="color:blue">See a
						sample</a>
				</div> -->
			</div>

			<div class="row justify-content-center">

				<div class="col-md-7 col-lg-5" style="padding-right:0px;padding-left:0px">

					<div class="wrap">
						<div class="w3-sidebar w3-bar-block w3-border-right" style="display:none" id="mySidebar">
							 <button onclick="w3_close()" class="w3-bar-item w3-large">Close</button>
							<a href="#" onclick="showReport()" class="w3-bar-item w3-button">VIN Report</a>
							<a href="#" onclick="showCredits()" class="w3-bar-item w3-button">Add Credits</a>
							<a href="#" onclick="showSettings()" class="w3-bar-item w3-button">Settings </a>
							<a href="#" onclick="showSupport()" class="w3-bar-item w3-button">Support </a>
							<a href="/signout" class="w3-bar-item w3-button">Signout </a>
						</div>

						<div class="w3-teal" style="width:100%">
							<button class="w3-button w3-teal w3-xlarge" onclick="w3_open()">&#9776;</button>

							<h3 class="mb-4" style="display:inline; color:white;" id="pageTitle">Auto VIN Report </h3>

						</div>
						<!-- <div class="img" style="background-image: url(/public/images/bg-1.jpg);"></div> -->


						<div onclick="w3_close()">
							<div id="report" class="login-wrap p-4 p-md-5">

								<!-- <div class="d-flex">
									<div class="w-100">
										<h3 class="mb-4">AutoCheck Report </h3>
									</div>
								</div> -->

								<div style="display: flex;justify-content: right;font-weight: bold;">

									<div>
										My Credits : <span class="mycredits">
											<%= credits %>
										</span>
									</div>
								</div>

								<!-- <form action="/autocheck?username=<%=username %>" class="signin-form" method="post"> -->
								<div class="form-group mt-3 " style="margin-top:40px">
									<label class="label-login" for="vin">VIN</label>
									<input type="text" class="form-control" id="vin" name="vin" placeholder="VIN"
										required>
								</div>
								<div class="form-group " style="margin-top:40px">
									<input type="email" class="form-control" id="email" name="email" value="<%=email %>"
										required>
									<label class="form-control-placeholder" for="email">Email</label>
								</div>


								<div class="form-group">
									<button type="submit" class="form-control btn btn-primary rounded submit px-3"
										onclick="getreport()">Buy Report</button>
								</div>

								<div id="report_download" >

								</div>

								<!-- </form> -->
								<div>
									5 credits (or $5) per report
								</div>
								<div id="report_message" style="font-weight:nold; color:red">
									<%= message %>
								</div>
								<!-- <p class="text-center">Already a member? <a data-toggle="tab" href="#" onclick="showSignIn()">Sign In</a></p> -->
							</div>

							<div id="credits" class="login-wrap p-4 p-md-5" style="display: none;">

								<!-- <div class="d-flex">
									<div class="w-100">
										<h3 class="mb-4">AutoCheck Report </h3>
									</div>
								</div> -->


								<div style="text-align: right; font-weight: bold;">
									My Credits : <span class="mycredits">
										<%= credits %>
									</span>
								</div>


								<form id="credits-form" action="/addcredits?username=<%=username %>" class="signin-form"
									method="post">
									<div class="form-group mt-3 " style="margin-top:40px">
										<label for="credits_add" class="label-login">Credits to add</label>
										<input type="number" max="300" value="10" class="form-control"
											id="credits_add" name="credits_add" placeholder="Credits to add" required>
									</div>
									<div>
										$1 per credit and 5 credits per report
									</div>

									<input id="verify_payment" class="form-control" type="hidden" name="verify_payment"
										value="<%= verifypayment%>">

									<div class="form-group" id="paypal" style="margin-top:50px">
										<!-- <button type="submit" class="form-control btn btn-primary rounded submit px-3">Add
										Credits</button> -->
									</div>


								</form>

								<div id="credits_message" style="font-weight:nold; color:red">
									<%= message %>
								</div>
								<!-- <p class="text-center">Already a member? <a data-toggle="tab" href="#" onclick="showSignIn()">Sign In</a></p> -->

								<input id="username" type="hidden" value="<%=username %>">
							</div>

							<div id="settings" class="login-wrap p-4 p-md-5" style="display:none;">
								<form action="/password-reset-internal" class="signin-form" method="post"
									onsubmit="return validateConfirmPassword()">
									<div class="form-group mt-3">
										<label class="label-login" for="username">Username</label>
										<input type="text" class="form-control" id="username" name="username" readonly
											value="<%= username %>" required>
									</div>
									<div class="form-group">
										<input type="password" class="form-control" id="currentpassword"
											name="currentpassword" required>
										<label class="form-control-placeholder" for="currentpassword">Current
											Password</label>
									</div>

									<div class="form-group">
										<input type="password" class="form-control" id="newpassword" name="newpassword"
											pattern="^(?=.*[a-z])(?=.*[A-Z])(?=.*\d)[a-zA-Z\d]{8,}$|^(?=.*[A-Za-z])(?=.*\d)(?=.*[$@$!%*#?&])[A-Za-z\d$@$!%*#?&]{8,}$"
											title="Minimum eight characters, at least one uppercase letter, one lowercase letter and one number"
											required>
										<label class="form-control-placeholder" for="newpassword">New Password</label>
										<span toggle="#newpassword"
											class="fa fa-fw fa-eye field-icon toggle-password"></span>
									</div>
									<div class="form-group">
										<input type="password" class="form-control" id="confirm_password"
											name="confirm_password" required>
										<label class="form-control-placeholder" for="confirm_password">Confirm New
											Password</label>
									</div>

									<div class="form-group">
										<button type="submit"
											class="form-control btn btn-primary rounded submit px-3">Reset
											Password</button>
									</div>
								</form>

								<div id="note" style="font-weight:nold; color:red"></div>
							</div>

							<div id="support" class="login-wrap p-4 p-md-5" style="display:none;">

								<!-- <div class="d-flex">
									<div class="w-100">
										<h3 class="mb-4">AutoCheck Report </h3>
									</div>
								</div> -->

								<div style="display: flex;justify-content: right;font-weight: bold;">

								</div>

								<!-- <form action="/autocheck?username=<%=username %>" class="signin-form" method="post"> -->
								<div class="form-group mt-3 " style="margin-top:40px">
									<label class="label-login" for="vin">Contact support:</label>

									support@thevinreport.com

								</div>

								<div class="form-group">
									<a href='mailto:support@thevinreport.com?subject=Contact from user <%=username%>'> <button type="submit" class="form-control btn btn-primary rounded submit px-3">Email Support</button>
									</a>
									</div>
								<!-- </form> -->
								<div>
									
								</div>
								<div id="support_message" style="font-weight:nold; color:red">
								
								</div>
								<!-- <p class="text-center">Already a member? <a data-toggle="tab" href="#" onclick="showSignIn()">Sign In</a></p> -->
							</div>



						</div>

					</div>
				</div>
			</div>
		</div>
	</section>

<div id="reportdata" style="display: none;">

</div>

</body>

</html>